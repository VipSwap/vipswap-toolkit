    "@types/styled-system": "^5.1.10",
    "lodash": "^4.17.20",
    "react-feather": "^2.0.9",
    "react-inlinesvg": "^2.2.2",
    "react-popper": "^2.2.5",
    "react-svg": "^13.0.3",
